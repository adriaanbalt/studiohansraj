{{> head contact.head }}

{{> navigation-mobile }}
<div id='content'>
	<header>
		{{> logo }}
		{{> navigation }}
	</header>
	<div id='contact'>
		<div class='container'>
			<img src="{{contact.content.image }}">
		</div>
		<div class="container">
			<div>{{{ contact.content.about }}}</div>
			<p>{{{ contact.content.address }}}</p>
			<p>
				<a href='tel:{{ contact.content.telephone }}'>{{ contact.content.telephone }}</a>
			</p>
			<p class="email">
				<a href='mailto:{{ contact.content.email }}'>{{ contact.content.email }}</a>
			</p>
		</div>

		<div class="container logos">
			<ul>
			{{#each contact.content.client-list}}
				<li>
					<img src="{{this}}"/>
				</li>
			{{/each}}
			</ul>
		</div>
	</div>
	{{> footer }}
</div>

{{> foot contact.foot }}
